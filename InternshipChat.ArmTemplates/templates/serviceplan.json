{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "internshipchatAppBaseName": {
      "type": "string"
    },
    "internshipchatAppBaseSkuName": {
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "name": "[parameters('internshipchatAppBaseName')]",
      "type": "Microsoft.Web/serverfarms",
      "location": "[resourceGroup().location]",
      "apiVersion": "2021-02-01",
      "sku": {
        "name": "[parameters('internshipchatAppBaseSkuName')]"
      },
      "dependsOn": [],
      "tags": {
        "displayName": "internshipchatAppBase"
      },
      "properties": {
        "name": "[parameters('internshipchatAppBaseName')]",
        "numberOfWorkers": 1
      }
    }
  ],
  "outputs": {
    "planId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Web/serverfarms', parameters('internshipchatAppBaseName'))]"
    }
  }
}
